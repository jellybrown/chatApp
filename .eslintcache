</div>\n        );\n    }\n}\n\nexport default MessageHeader;","/Users/kimyujin/Documents/react_first/react-chat-app/src/redux/reducers/chatRoom_reducer.js",[],"/Users/kimyujin/Documents/react_first/react-chat-app/src/components/ChatPage/MainPanel/message.jsx",[],"/Users/kimyujin/Documents/react_first/react-chat-app/src/redux/actions/chatRoom_action.js",[],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"109","replacedBy":"113"},{"ruleId":"111","replacedBy":"114"},{"ruleId":"109","replacedBy":"115"},{"ruleId":"111","replacedBy":"116"},{"ruleId":"109","replacedBy":"117"},{"ruleId":"111","replacedBy":"118"},{"ruleId":"109","replacedBy":"119"},{"ruleId":"111","replacedBy":"120"},{"ruleId":"121","severity":1,"message":"122","line":14,"column":12,"nodeType":"123","messageId":"124","endLine":14,"endColumn":19},{"ruleId":"121","severity":1,"message":"125","line":9,"column":8,"nodeType":"123","messageId":"124","endLine":9,"endColumn":14},"no-native-reassign",["126"],"no-negated-in-lhs",["127"],["126"],["127"],["126"],["127"],["126"],["127"],["126"],["127"],"no-unused-vars","'loading' is assigned a value but never used.","Identifier","unusedVar","'Button' is defined but never used.","no-global-assign","no-unsafe-negation"]